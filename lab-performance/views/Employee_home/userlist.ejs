<!DOCTYPE html>
<html>
<head>
	<title>User List</title>
</head>
<body>
	<h1> Employee List</h1>

	<a href="/Admin_home">Back</a> | 
	<a href="/logout">logout</a> 
	<br>

	<table border="1">
		<tr>
			<td>Company Name</td>
			<td>Title</td>
			<td>location</td>
			<td>Salary</td>
			
		</tr>
		

		<% 
			jobs.forEach( function(std){ %>
			<tr>
				<td><%= std.companyname %></td>
				<td><%= std.title %></td>
				<td><%= std.location %></td>
				<td><%= std.salary %></td>
				<td>
					<a href="/user/edit/<%= std.id %>">Edit</a> |
					<a href="/user/delete/<%= std.id %>">Delete</a>
				</td>
			</tr>

		<%
			}); 
		%>
		
	</table>

</body>
</html>